public class PartLargeDelete extends BaseResource{
    @Post
    public String doGet(){
        DataPackage dp = new DataPackage();
        PartAction a = new PartAction();
        dp.setStatus(a.deleteLarge(form));
        return dp.toJSON();
    }
}
