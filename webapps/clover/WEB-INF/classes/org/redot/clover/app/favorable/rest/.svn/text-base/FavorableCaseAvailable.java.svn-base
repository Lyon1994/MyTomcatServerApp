public class FavorableCaseAvailable extends BaseResource {
    @Get("json")
    public String doGet() {
        DataPackage dp = new DataPackage();
        FavorableAction a = new FavorableAction();
        dp.addTable("table", a.available());
        return dp.toJSON();
    }
