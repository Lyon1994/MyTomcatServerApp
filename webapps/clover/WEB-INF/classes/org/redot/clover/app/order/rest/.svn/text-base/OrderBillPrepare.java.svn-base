public class OrderBillPrepare extends BaseResource {

    @Post("json")
    public String doPost(){
        DataPackage dp = new DataPackage();
        OrderBillAction a = new OrderBillAction();
        dp.getData().put("form", a.prepare(form));
        return dp.toJSON();
    }
}
