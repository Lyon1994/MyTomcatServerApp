public class OrderReportCash extends BaseResource {

    @Get("json")
    public String doGet(){
        DataPackage dp = new DataPackage();
        OrderReportAction a = new OrderReportAction();
        dp.getData().put("form", a.cash());
        return dp.toJSON();
    }
}
