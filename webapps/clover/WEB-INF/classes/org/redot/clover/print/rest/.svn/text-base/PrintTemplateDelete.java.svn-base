public class PrintTemplateDelete extends BaseResource {



    @Post("json")
    public String doPost() {
        DataPackage dp = new DataPackage();
        PrintTemplateAction a = new PrintTemplateAction();
        dp.setStatus(a.delete(form));
        return dp.toJSON();
    }

